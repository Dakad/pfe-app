
block content
  -var requestMethod = format['request-method']
  .panel.panel-info
    .panel-heading
      h3.panel-title#format Format 
    .panel-body
      h2=format['request-method'].title
      each val in format['request-method']['description']
        p=val
      table.table.table-hover.table-striped
        thead
          tr.active.text-center
          each head in format['request-method'].content.headers
              th=head
          tbody
          each row in format['request-method'].content.body
            tr
            each val in row
              td=val

      h2=format['response-status-code'].title
      each val in format['response-status-code']['description']
        p=val
      table.table.table-hover.table-striped
        thead
          tr.active.text-center
          each head in format['response-status-code'].content.headers
              th=head
          tbody
          each row in format['response-status-code'].content.body
            tr
            each val in row
              td=val


      h2=format['response-data'].title
      each val in format['response-data'].description
        p=val
      pre=format['response-data'].code.toString()

    